@model IEnumerable<ItCareer.Models.Dog>

@{
    Layout = "_Layout";
}

@foreach(var dog in @Model)
{
    <h5>Name: @dog.Name</h5>
    <h5>Breed: @dog.Breed</h5>
    <h5>Age: @dog.Age</h5>
    <a class="btn" asp-action="Edit" asp-route-id="@dog.Id">Edit</a>
    <a class="btn" asp-action="Delete" asp-route-id="@dog.Id">Delete</a>
}

<a class="btn" asp-action="Create">Create</a>